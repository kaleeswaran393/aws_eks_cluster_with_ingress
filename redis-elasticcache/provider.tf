provider "aws" {
    profile = var.profile
    region  = var.region
    token      = "terraform"   
    shared_credentials_files = ["~/.aws/credentials"]

}
terraform {
  required_version = ">= 1.3.2"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = ">= 5.61"
    }
    tls = {
      source  = "hashicorp/tls"
      version = ">= 3.0"
    }
    time = {
      source  = "hashicorp/time"
      version = ">= 0.9"
    }
  }
}